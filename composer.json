{
    "name": "rabble/skeleton",
    "license": "MIT",
    "type": "project",
    "description": "Project template for starting a project based on the Rabble admin system.",
    "homepage": "https://rabble.rocks/",
    "keywords": [
        "symfony",
        "rabble",
        "cms",
        "web-application",
        "platform",
        "data-managment",
        "project-template"
    ],
    "authors": [
        {
            "name": "Rachel Snijders",
            "email": "rainbowvision.bn@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/Rabble-Rocks/rabble-skeleton/issues",
        "source": "https://github.com/Rabble-Rocks/rabble-skeleton"
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "monolog/monolog": "1.25.5 as 1.24.0",
        "rabble/admin-bundle": "1.0.x-dev",
        "rabble/content-bundle": "1.0.x-dev",
        "rabble/datatable-bundle": "1.0.x-dev",
        "rabble/field-type-bundle": "1.0.x-dev",
        "rabble/menu-bundle": "1.0.x-dev",
        "rabble/seo-bundle": "1.0.x-dev",
        "rabble/snippet-bundle": "1.0.x-dev",
        "rabble/user-bundle": "1.0.x-dev",
        "rabble/website-bundle": "1.0.x-dev",
        "rabble/preview-bundle": "1.0.x-dev",
        "stof/doctrine-extensions-bundle": "^1.4",
        "symfony/asset": "^5",
        "symfony/console": "^5",
        "symfony/dotenv": "^5",
        "symfony/expression-language": "^5",
        "symfony/flex": "^1.3.1",
        "symfony/form": "^5",
        "symfony/framework-bundle": "^5",
        "symfony/http-client": "^5",
        "symfony/intl": "^5",
        "symfony/mailer": "^5",
        "symfony/mime": "^5",
        "symfony/monolog-bundle": "^3.1",
        "symfony/notifier": "^5",
        "symfony/orm-pack": "*",
        "symfony/process": "^5",
        "symfony/security-bundle": "^5",
        "symfony/serializer-pack": "*",
        "symfony/string": "^5",
        "symfony/translation": "^5",
        "symfony/twig-pack": "*",
        "symfony/validator": "^5",
        "symfony/web-link": "^5",
        "symfony/yaml": "^5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "*",
        "phpstan/phpstan": "^0.12.91",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*"
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "platform": {
            "php": "7.4.0"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "post-create-project-cmd": [
            "@php -r \"file_put_contents('.gitignore', str_replace(['composer.lock' . PHP_EOL, 'symfony.lock' . PHP_EOL], ['', ''], file_get_contents('.gitignore')));\"",
            "@php -r \"copy('.env.dist', '.env.local');\""
        ]
    },
    "conflict": {
        "symfony/symfony": "*",
        "symfony/web-profiler-bundle": "5.3.4"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false
        }
    }
}
